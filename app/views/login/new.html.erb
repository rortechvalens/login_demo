<h1>Login Hear </h1>

<%= form_for @user , :as => :user, :url => login_create_path, :method => "post" do |f| %>

<p>
  <label>Enter Email</label><br/>
  <%= f.text_field :email, :required => true %>
</p>

<p>
  <label>Enter Password</label><br/>
  <%= f.password_field :password , :required => true %>
</p>

<p>
  <%= f.submit :Login %>
</p>

<% end %>